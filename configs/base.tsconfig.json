{
    "compilerOptions": {
        "target": "ES2017",
        "module": "NodeNext",
        "lib": [
            "ESNext"
        ],
        "sourceMap": true,
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noImplicitOverride": true,
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "declaration": true,
        "allowSyntheticDefaultImports": true
    }
}